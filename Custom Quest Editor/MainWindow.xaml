                                    <ComboBox Grid.Column="6" Name="Stars" Padding="0,5" Margin="0,5"/>
                                    <Label Grid.Row="1" Grid.Column="0" Content="Rank"/>
                                    <Label Grid.Row="1" Grid.Column="4" Content="Map"/>
                                    <Label Grid.Row="2" Grid.Column="0" Content="Player Spawn"/>
