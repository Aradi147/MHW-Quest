                                    <Label Grid.Row="3" Grid.Column="0" Content="Time Cycle"/>
                                    <Label Grid.Row="3" Grid.Column="4" Content="Weather Condition"/>
                                    <Label Grid.Row="4" Grid.Column="0" Content="Multiplayer Modifier"/>
                                    <Label Grid.Row="4" Grid.Column="4" Content="Quest Timer"/>
                                    <Label Grid.Row="5" Grid.Column="0" Content="Reward Money"/>
                                    <Label Grid.Row="5" Grid.Column="4" Content="Faint Penalty"/>
                                    <Label Grid.Row="6" Grid.Column="0" Content="HR Requirement"/>
                                    <Label Grid.Row="6" Grid.Column="4" Content="Background Music"/>
                                    <Label Grid.Row="7" Grid.Column="0" Content="Quest Type"/>
